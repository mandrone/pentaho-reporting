<meta-data xmlns="http://reporting.pentaho.org/namespaces/engine/classic/metadata/1.0">

  <include-globals src="res://org/pentaho/reporting/engine/classic/core/metadata/global-meta-elements.xml"/>
  
  <element name="survey-scale" expert="false" hidden="false" preferred="false"
           bundle-name="org.pentaho.reporting.engine.classic.core.modules.misc.survey.metadata"
           implementation="org.pentaho.reporting.engine.classic.core.modules.misc.survey.SurveyScaleType">

    <attribute-group-ref ref="common-attributes"/>
    <attribute-group-ref ref="interactivity"/>
    <attribute-group-ref ref="field-value"/>
    <attribute-group-ref ref="static-computed-value"/>
    <attribute-group-ref ref="replaced-content-attributes"/>

    <attribute namespace="http://reporting.pentaho.org/namespaces/engine/classic/extensions/survey-scale/1.0"
               name="lowest"
               mandatory="false"
               expert="false"
               hidden="false"
               value-type="java.lang.Number"
               value-role="Value"/>

    <attribute namespace="http://reporting.pentaho.org/namespaces/engine/classic/extensions/survey-scale/1.0"
               name="highest"
               mandatory="false"
               expert="false"
               hidden="false"
               value-type="java.lang.Number"
               value-role="Value"/>

    <attribute namespace="http://reporting.pentaho.org/namespaces/engine/classic/extensions/survey-scale/1.0"
               name="range-lower-bound"
               mandatory="false"
               expert="false"
               hidden="false"
               value-type="java.lang.Number"
               value-role="Value"/>

    <attribute namespace="http://reporting.pentaho.org/namespaces/engine/classic/extensions/survey-scale/1.0"
               name="range-upper-bound"
               mandatory="false"
               expert="false"
               hidden="false"
               value-type="java.lang.Number"
               value-role="Value"/>

    <style-group-ref ref="borders"/>
    <style-group-ref ref="common"/>
    <style-group-ref ref="replaced-content"/>
    <style-group-ref ref="layout"/>
  </element>

</meta-data>